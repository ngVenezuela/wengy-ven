    "angular",
    "angularjs"
  ],
  "author": "Leonardo Cabeza <info@leonardocabeza.com> (https://github.com/leocabeza)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngVenezuela/wengy-ven/issues"
  },
  "homepage": "https://github.com/ngVenezuela/wengy-ven#readme",
  "dependencies": {
    "@sentry/node": "^4.5.3",
    "body-parser": "^1.18.3",
    "cron": "^1.6.0",
    "dialogflow": "^0.8.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "node-fetch": "^2.3.0",
    "node-telegram-bot-api": "^0.30.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "ngrok": "^3.1.1",
    "prettier": "1.16.4"
  },
  "husky": {
